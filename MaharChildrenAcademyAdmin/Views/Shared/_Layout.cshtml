<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>MAHAR ADMIN - @ViewBag.Title</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/assets/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/assets/vendors/css/vendor.bundle.base.css">
    <!-- Layout styles -->
    <link rel="stylesheet" href="~/assets/css/style.css">
    <!-- End layout styles -->
    <link href="~/fonts/fontawesome-free/css/all.min.css" rel="stylesheet" />
    <link href="~/fonts/fontawesome-free/css/fontawesome.min.css" rel="stylesheet" />
    <link rel="shortcut icon" href="~/assets/images/Mahar-children-academy-Logo.png" />
</head>
<body>
    <div class="container-scroller">
        @Html.Partial("_Navbar")
        <div class="container-fluid page-body-wrapper">
            @Html.Partial("_Sidebar")

            <div class="main-panel">
                <div class="content-wrapper">
                    @RenderBody()
                </div>
                @Html.Partial("_Footer")
            </div>
        </div>
    </div>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery-3.6.0.min.js"></script>
    <script src="~/fonts/fontawesome-free/js/all.min.js"></script>
    <script src="~/fonts/fontawesome-free/js/fontawesome.min.js"></script>
    <script src="~/assets/vendors/js/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page -->
    <script src="~/assets/vendors/chart.js/Chart.min.js"></script>
    <!-- End plugin js for this page -->
    <!-- inject:js -->
    <script src="~/assets/js/off-canvas.js"></script>
    <script src="~/assets/js/hoverable-collapse.js"></script>
    <script src="~/assets/js/misc.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page -->
    <script src="~/assets/js/dashboard.js"></script>
    <script src="~/assets/js/todolist.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
